<?xml version="1.0" encoding="utf-8" ?>
<Autodiscover xmlns="http://schemas.microsoft.com/exchange/autodiscover/responseschema/2006">
	<Response xmlns="http://schemas.microsoft.com/exchange/autodiscover/outlook/responseschema/2006a">
		<User>
			<DisplayName>Excision Mail - {{ domains[0].name }}</DisplayName>
		</User>
		<Account>
			<AccountType>email</AccountType>
			<Action>settings</Action>
			<ServiceHome>https://autodiscover.{{ domains[0].name }}</ServiceHome>
			<Protocol>
				<Type>IMAP</Type>
				<Server>{{ mail_subdomain }}.{{ domains[0].name }}</Server>
				<Port>993</Port>
				<LoginName></LoginName>
			</Protocol>
			<Protocol>
				<Type>SMTP</Type>
				<Server>{{ mail_subdomain }}.{{ domains[0].name }}</Server>
				<Port>465</Port>
				<LoginName></LoginName>
			</Protocol>
		</Account>
	</Response>
</Autodiscover>
